    "date-holidays": "^1.4.3",
    "easy-pdf-merge": "^0.2.2",
    "html-pdf": "^2.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "nunjucks": "^3.2.0"
  },
  "devDependencies": {
